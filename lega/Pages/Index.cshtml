@page
@using Microsoft.AspNetCore.Localization
@model IndexModel
@{
    ViewData["Title"] = "Home page";
    var requestCultureFeature = HttpContext.Features.Get<IRequestCultureFeature>();
    var requestCulture = requestCultureFeature.RequestCulture;
}



<!--Carousel-Section-Start-->
<section id="slider">
    <div id="home-carousel" class="carousel slide" data-ride="carousel">
        <div class="carousel-inner">

            @foreach (var item in Model.CarouselList)
            {
                <div class="item @item.State" style="background-image:url(@item.ImageUrl)">
                    <div class="carousel-caption container">
                        <div class="row">
                            <div class="col-md-12 col-sm-12 col-xs-12" style="padding-left:7mm">
                                @if (requestCulture.Culture.Name.ToString() == "hy-AM" || (requestCulture.Culture.Name.ToString() == "hy"))
                                {
                                    <h1>@item.Title</h1>
                                    <h2>@item.Name</h2>
                                    <p>@item.Context</p>
                                }

                                else
                                             @if (requestCulture.Culture.Name.ToString() == "ru-RU" || (requestCulture.Culture.Name.ToString() == "ru"))
                                {
                                    <h1>@item.TitleRu</h1>
                                    <h2>@item.NameRu</h2>
                                    <p>@item.ContextRu</p>
                                }

                                else
                                {
                                    <h1>@item.TitleEn</h1>
                                    <h2>@item.NameEn</h2>
                                    <p>@item.ContextEn</p>
                                }
                            </div>
                        </div>
                    </div>

                </div>
            }
            <a class="home-carousel-left" href="#home-carousel" data-slide="prev"><i class="fa fa-angle-left"></i></a> <a class="home-carousel-right" href="#home-carousel" data-slide="next"><i class="fa fa-angle-right"></i></a>
        </div>
    </div>
    <!--/#home-carousel-->
    <br />
    <hr />
    <br />
</section>

<!--About-Section-Start-->
<section id="about">
    <div class="container">
        @if (requestCulture.Culture.Name.ToString() == "hy-AM" || (requestCulture.Culture.Name.ToString() == "hy"))
        {
            <div class="col-md-8 col-md-offset-2">
                <div class="heading">
                    <h2>ՄԵՐ <span>ՄԱՍԻՆ</span></h2>
                    <div class="line"></div>
                    <p style="font-size:20px">@Model.About.Title</p>
                </div>
            </div>
            <div class="row">
                <div class="col-md-12 ab-sec">
                    <div class="col-md-6">
                        <h3 class="wow fadeInDown" data-wow-duration="1000ms" data-wow-delay="300ms"><span>@Model.About.Name</span></h3>
                        <br>
                        <p>@Model.About.Context</p>
                    </div>
                    <div class="col-md-6 ab-sec-img wow fadeInUp" data-wow-duration="1000ms" data-wow-delay="300ms"><img style="margin-top:20px" height="290" src="@Model.About.ImageUrl" alt=""> </div>
                </div>
            </div>
        }

        else

        @if (requestCulture.Culture.Name.ToString() == "ru-RU" || (requestCulture.Culture.Name.ToString() == "ru"))
        {
            <div class="col-md-8 col-md-offset-2">
                <div class="heading">
                    <h2>О <span>НАС</span></h2>
                    <div class="line"></div>
                    <p style="font-size:20px">@Model.About.TitleRu</p>
                </div>
            </div>
            <div class="row">
                <div class="col-md-12 ab-sec">
                    <div class="col-md-6">
                        <h3 class="wow fadeInDown" data-wow-duration="1000ms" data-wow-delay="300ms"><span>@Model.About.NameRu</span></h3>
                        <br>
                        <p>@Model.About.ContextRu</p>
