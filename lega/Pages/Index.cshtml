                    <h2>PRICE PACKAG<span>ES</span></h2>
                    <div class="line"></div>
                    <p style="font-size:15px"><span><strong>Choose</strong></span> the package of services that suits you or contact us to make an individual package</p>
                </div>
            </div>
            <div class="row">
                @{
                    var delay = 0;
                }
                @foreach (var item in Model.PricingList)
                {
                    var delaySec = @delay.ToString() + "ms";
                    <div class="col-sm-6 col-md-3">
                        <div class="wow zoomIn" data-wow-duration="400ms" data-wow-delay="@delaySec">
                            <ul class="pricing">
                                <li class="plan-header">
                                    <div class="price-duration">
                                        @if (!String.IsNullOrWhiteSpace(item.Price))
                                        {
                                            <div class="price"> @item.Price  </div>
                                            <div class="duration"> @item.PriceValueEn  </div>
                                        }
                                        else
                                        {
                                            <div class="price"></div>
                                            <div class="duration" style="font-size:12px;padding-top:12px"> @item.PriceValueEn  </div>
                                        }
                                    </div>
                                    <div class="plan-name"> @item.NameEn </div>
                                </li>
                                <li>Up to <strong>@item.EmployeCount</strong> employees</li>
                                <li>more than <strong>@item.PriceTextEn</strong>  AMD turnover</li>
                                <li>@item.ContextEn</li>
