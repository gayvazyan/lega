                        <div class="col-md-4 news-sec">
                            <div class="news-info">
                                <img src="@item.ImageUrl" class="img-responsive">
                                <div class="data-meta">
                                    <h4>@CultureInfo.CreateSpecificCulture("en-US").DateTimeFormat.GetAbbreviatedMonthName(item.Date.Month)</h4>
                                    <strong>@item.Date.Day</strong><br>
                                    @item.Date.Year
                                </div>
                                <h5>@item.TitleEn</h5>
                                <h6><i class="fa fa-pencil"></i> @item.AuthorEn</h6>
                                <p>@item.ShortContextEn</p>
                                <a href="/Newss/Item/@item.Id/" class="btn-blg">Read More</a>
                            </div>
                        </div>
                    }

                </div>
            </div>
        }


    </div>
</section>

