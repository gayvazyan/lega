                    <div class="line"></div>
                    <p style="font-size:15px"><span><strong>Выбирать</strong></span> удобный для вас пакет услуг или свяжитесь с нами для составления индивидуального пакета</p>
                </div>
            </div>
            <div class="row">
                @{
                    var delay = 0;
                }
                @foreach (var item in Model.PricingList)
                {
                    var delaySec = @delay.ToString() + "ms";
                    <div class="col-sm-6 col-md-3">
                        <div class="wow zoomIn" data-wow-duration="400ms" data-wow-delay="@delaySec">
                            <ul class="pricing">
                                <li class="plan-header">
                                    <div class="price-duration">
                                        @if (!String.IsNullOrWhiteSpace(item.Price))
                                        {
                                            <div class="price"> @item.Price  </div>
                                            <div class="duration"> @item.PriceValueRu  </div>
                                        }
                                        else
                                        {
                                            <div class="price"></div>
                                            <div class="duration" style="font-size:12px;padding-top:12px"> @item.PriceValueRu  </div>
                                        }
                                    </div>
                                    <div class="plan-name"> @item.NameRu </div>
                                </li>
                                <li>Более <strong>@item.EmployeCount</strong> сотрудников</li>
                                <li>Оборот более <strong>@item.PriceTextRu</strong> AMD</li>
                                <li>@item.ContextRu</li>
                            </ul>
                        </div>
                    </div>
                    delay += 200;
                }
            </div>
        }
        else

        {
