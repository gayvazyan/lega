<!--Pricing-Section-Start-->
<section id="pricing">
    <div class="container">

        @if (requestCulture.Culture.Name.ToString() == "hy-AM" || (requestCulture.Culture.Name.ToString() == "hy"))
        {
            <div class="col-md-8 col-md-offset-2">
                <div class="heading">
                    <h2>ԳՆԱՅԻՆ ՓԱԹԵԹ<span>ՆԵՐ</span></h2>
                    <div class="line"></div>
                    <p style="font-size:15px"><span><strong>Ընտրեք</strong></span> ձեզ հարմար ծառայությունների փաթեթը կամ կապվեք մեզ հետ ինդիվիդուալ փաթեթ կազմելու համար</p>
                </div>
            </div>
            <div class="row">
                @{
                    var delay = 0;
                }
                @foreach (var item in Model.PricingList)
                {
                    var delaySec = @delay.ToString() + "ms";
                    <div class="col-sm-6 col-md-3">
                        <div class="wow zoomIn" data-wow-duration="400ms" data-wow-delay="@delaySec">
                            <ul class="pricing">
                                <li class="plan-header">
                                    <div class="price-duration">
                                        @if (!String.IsNullOrWhiteSpace(item.Price))
                                        {
                                            <div class="price"> @item.Price  </div>
                                            <div class="duration"> @item.PriceValue  </div>
                                        }
                                        else
                                        {
                                            <div class="price"></div>
                                            <div class="duration" style="font-size:12px;padding-top:12px"> @item.PriceValue  </div>
                                        }
                                    </div>
                                    <div class="plan-name"> @item.Name </div>
                                </li>

                                @if (!String.IsNullOrWhiteSpace(item.Price))
                                {
                                    <li>Մինչև <strong>@item.EmployeCount</strong> աշխատակից</li>
                                    <li><strong>Մինչև @item.PriceText</strong>  դրամ շրջանառություն</li>
                                }
                                else
                                {
                                    <li><strong>@item.EmployeCount</strong> -ից ավել աշխատակից</li>
                                    <li><strong> @item.PriceText</strong> -ից ավել շրջանառություն</li>
                                }


                                <li>@item.Context</li>
                            </ul>
                        </div>
                    </div>
                    delay += 200;
                }
            </div>
        }

        else

        @if (requestCulture.Culture.Name.ToString() == "ru-RU" || (requestCulture.Culture.Name.ToString() == "ru"))
        {
            <div class="col-md-8 col-md-offset-2">
                <div class="heading">
